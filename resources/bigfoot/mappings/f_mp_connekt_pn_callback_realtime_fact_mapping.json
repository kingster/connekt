{
	"dynamic_templates": [{
		"properties_template": {
			"mapping": {
				"index": "not_analyzed",
				"type": "{dynamic_type}"
			},
			"path_match": "*"
		}
	}],
	"_source": {
		"enabled": "true"
	},
	"_ttl": {
		"enabled": "true",
		"default": 1296000000
	},
	"properties": {
		"app_name": {
			"index": "not_analyzed",
			"type": "string"
		},
		"context_id": {
			"index": "not_analyzed",
			"type": "string"
		},
		"event_type": {
			"index": "not_analyzed",
			"type": "string"
		},
		"platform": {
			"index": "not_analyzed",
			"type": "string"
		},
		"timestamp": {
			"format": "dateOptionalTime",
			"type": "date"
		}
	}
}